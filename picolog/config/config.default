[server]
host = localhost
port = 50000
max_connections = 5
log_format = %(asctime)s - %(name)s - %(levelname)s - %(message)s

[adc]
conversion_time = 2
sample_time = 1
socket_buffer_length = 1000
fetch_delay = 15
max_adc_connection_attempts = 10
min_adc_reconnection_delay = 0.5
hrdl_library_path = /opt/picoscope/lib/libpicohrdl.so
hrdl_string_buffer_length = 1000
hrdl_sample_buffer_length = 1000

[datastore]
max_readings = 1000000
